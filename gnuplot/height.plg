set terminal png size 1024,768

set output "out/height_plot.png"

set datafile separator ";"

set view map
set pm3d
set palette gray
set pm3d interpolate  2,2
set pm3d at b

set samples 100; set isosamples 100

if (!exists("filename")) filename='out/h.csv'
plot filename using 3:4:2 with points palette pointsize 2 pointtype 7 title "height"
